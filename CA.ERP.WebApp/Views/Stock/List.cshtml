@{
    @model CA.ERP.WebApp.ReportDto.StockList;
}


<div class="page container column">
    <div class="container column center">
        <img class="logo item" src="@ViewData["WebAppUrl"]/images/logo.png" />
        <span>@Model.BranchName</span>
        <span>@Model.BranchContact</span>



    </div>
    <div class="container">
        <div class="box"><span></span></div>
        <div class="box"><span><h2>Product List</h2></span></div>

        <div class="box"><span><h3>@Model.Date.ToString(ViewBag.DateFormat)<h3></span></div>
    </div>
    <table>
        <thead>
            <tr>
                <th>Stock Number</th>
                <th>Brand Name</th>
                <th>Model </th>
                <th>Serial Number</th>
                <th>Status</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var stock in Model.Stocks)
            {
            <tr>
                <td>@stock.StockNumber</td>
                <td>@stock.BrandName</td>
                <td>@stock.Model</td>
                <td>@stock.SerialNumber</td>
                <td>@stock.Status</td>
            </tr>
            }

        </tbody>


    </table>
</div>